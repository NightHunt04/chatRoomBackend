{
    "version": 2,
    "builds": [
      {
        "src": "./index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/socket.io/(.*)",
        "dest": "./index.js",
        "headers": {
                "Access-Control-Allow-Origin": "*",
                "Access-Control-Allow-Methods": "GET,POST,PUT,DELETE,OPTIONS,HEAD",
                "Access-Control-Allow-Headers": "content-type"
        }
      },
      {
        "src": "/(.*)",
        "dest": "./index.js"
      }
    ]
  }